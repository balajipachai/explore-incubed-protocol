        "tags": ["Zongo AddTools"],
        "parameters": [
          {
            "name": "Add Punch Tool",
            "in": "body",
            "description": "Provide tool details",
            "schema": {
              "$ref": "#definitions/ADDTOOLS"
            }
          }
        ],
        "responses": {}
      }
    }
  },
  "definitions": {
    "RESPONSE": {
      "properties": {
        "status": {
          "type": "string",
          "example": "000"
        },
        "data": {
          "type": "object"
        },
        "error": {
          "type": "string",
          "example": ""
        },
        "message": {
          "type": "string",
          "example": ""
        }
      }
    },
    "ADDTOOLS": {
      "properties": {
        "sbr": {
          "type": "string",
          "example": "5 mm/inch"
        },
        "measuringUnit": {
          "type": "string",
          "example": "mm/inch"
        },
        "station": {
          "type": "string",
          "example": "A/B/C/D/E/F/Special_Case"
        },
        "shape": {
          "type": "string",
          "example": "round/square/reactangle/triangle/hexagon/octagon"
        },
        "toolLengthType": {
          "type": "string",
          "example": "long/short/insert"
        },
        "features": {
