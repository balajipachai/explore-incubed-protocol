          "example": "flat/roof_top/2_way_concave/4_way_concave/whisper/1_way/cup"
        },
        "price": {
          "type": "string",
          "example": "100$"
        },
        "make": {
          "type": "string"
        },
        "label": {
          "type": "string"
        },
        "location": {
          "type": "string"
        },
        "note": {
          "type": "string"
        },
        "dimension": {
          "type": "string"
        },
        "avatar": {
