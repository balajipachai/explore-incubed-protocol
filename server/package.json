{
  "name": "explore-incubed-protocol",
  "version": "1.0.0",
  "description": "Incubed is a protocol to access blockchain information (for instance Ethereum) decentralized with a stateless but full verifying client. Nodes participating in this open network are registered in a Registry Smart Contract.",
  "main": "app.js",
  "scripts": {
    "start:server": "node ./bin/www",
    "dev:server": "nodemon ./bin/www",
    "clean": "rm -rf .config .npm .node-gyp node_modules && npm install",
    "cleanSol": "rm -rf build/contracts",
    "compile": " truffle compile && npm run lint",
    "coverage": "truffle run coverage",
    "lint": "solhint \"contracts/**/*.sol\"",
    "migrate": " truffle migrate",
    "migrateAll": " truffle migrate --reset",
    "recompile": " npm run cleanSol && truffle compile --all && npm run lint",
    "test": "truffle test"
  },
  "private": true,
  "author": "Balaji Shetty Pachai <balaji.pachai08@gmail.com>",
  "dependencies": {
    "@openzeppelin/test-helpers": "^0.5.6",
    "app-root-path": "^3.0.0",
    "bignumber.js": "^9.0.0",
    "bluebird": "^3.7.2",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "ethereumjs-util": "^7.0.3",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "fix": "0.0.6",
    "hbs": "^4.1.1",
    "http-errors": "^1.8.0",
    "in3-wasm": "^2.5.4",
    "kafkajs": "^1.12.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "openzeppelin-solidity": "^2.5.0",
    "redis": "^3.0.2",
    "swagger-ui-express": "^4.1.4",
    "truffle": "^5.1.34",
    "truffle-flattener": "^1.4.2",
    "truffle-hdwallet-provider": "^1.0.17",
    "web3": "^1.2.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/node": "^7.10.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-bignumber": "^2.0.2",
    "codelyzer": "^5.1.2",
    "coveralls": "^3.0.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-mocha": "^7.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^1.4.0",
    "mocha": "^5.2.0",
    "solhint": "^2.3.0",
    "solidity-coverage": "^0.7.9"
  }
}
